<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apple.votingmachine">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.apple.votingmachine.RegisterActivity"/>
        <activity android:name="com.example.apple.votingmachine.LogInActivity"/>
        <activity android:name="com.example.apple.votingmachine.VotingRoomActivity"/>
        <activity android:name="com.example.apple.votingmachine.VotingRoomListActivity"/>
        <activity android:name="com.example.apple.votingmachine.AddVoterActivity"/>
        <activity android:name="com.example.apple.votingmachine.ProfileActivity"/>
        <activity android:name="com.example.apple.votingmachine.RequestActivty"/>
        <activity android:name="com.example.apple.votingmachine.RequestListActivity"/>
        <activity android:name="com.example.apple.votingmachine.SearchVotingRoomActivity"/>
        <activity android:name="com.example.apple.votingmachine.VotingRoomEditorActivity"/>
        <activity android:name="com.example.apple.votingmachine.MyVotingRoomActivity"/>
        <activity android:name="com.example.apple.votingmachine.MyProfileActivity"/>

    </application>

</manifest>